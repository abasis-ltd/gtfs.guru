{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Default capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "dialog:default",
        "dialog:allow-open",
        {
            "identifier": "shell:allow-open",
            "allow": [
                {
                    "pattern": "^.*"
                }
            ]
        }
    ]
}